{
	"name": "BlueSpiceWatchList",
	"version": "4.2.0-alpha",
	"url": "https://en.wiki.bluespice.com/wiki/Reference:BlueSpiceWatchList",
	"author": "Robert Vogel",
	"descriptionmsg": "bs-watchlist-desc",
	"type": "bluespice",
	"license-name": "GPL-3.0-only",
	"requires": {
		"MediaWiki": ">= 1.36.0",
		"extensions": {
			"BlueSpiceFoundation": ">= 4.1"
		}
	},
	"attributes": {
		"BlueSpiceFoundation": {
			"Extensions": {
				"BlueSpiceWatchList": {
					"className": "\\BlueSpice\\WatchList\\Extension",
					"extPath": "/BlueSpiceWatchList"
				}
			},
			"TagRegistry": {
				"watchlist": "\\BlueSpice\\WatchList\\Tag\\WatchList"
			}
		},
		"BlueSpiceUserSidebar": {
			"Widgets": {
				"WATCHLIST": {
					"callback": "\\BlueSpice\\WatchList\\Panel\\WatchList::factory",
					"default": true
				}
			}
		},
		"BlueSpiceDiscovery": {
			"AttentionIndicatorRegistry": {
				"watchlist": "\\BlueSpice\\WatchList\\AttentionIndicator\\WatchList::factory"
			}
		},
		"BlueSpiceVisualEditorConnector": {
			"TagDefinitions": [
				"ext.bluespice.watchlist.visualEditor"
			]
		},
		"ContentDroplets": {
			"Droplets": {
				"watchlist": {
					"class": "\\BlueSpice\\WatchList\\ContentDroplets\\WatchlistDroplet"
				}
			}
		}
	},
	"HookHandlers": {
		"skin": {
			"class": "\\BlueSpice\\WatchList\\HookHandler\\Skin"
		}
	},
	"Hooks": {
		"BeforePageDisplay": [
			"\\BlueSpice\\WatchList\\Hook\\BeforePageDisplay\\AddModules::callback"
		],
		"BSInsertMagicAjaxGetData": "\\BlueSpice\\WatchList\\Hook\\BSInsertMagicAjaxGetData\\AddWatchListTag::callback",
		"BSUsageTrackerRegisterCollectors": "\\BlueSpice\\WatchList\\Hook\\BSUsageTrackerRegisterCollectors\\AddWatchListTag::callback",
		"SkinTemplateNavigation::Universal": "skin"
	},
	"ResourceModules": {
		"ext.bluespice.watchlist.styles": {
			"styles": "bluespice.watchlist.css"
		},
		"ext.bluespice.watchlist.myWatchlistWindow.bootstrap": {
			"scripts": [
				"ext.bluespice.watchlist.myWatchlistWindow.bootstrap.js"
			]
		},
		"ext.bluespice.watchlist.myWatchlistWindow": {
			"styles": [
				"ext.bluespice.watchlist.myWatchlistWindow.css"
			],
			"messages": [
				"bs-watchlist-window-watchlist-title",
				"bs-watchlist-window-watchlist-intro",
				"bs-watchlist-grid-watchlist-column-link-header",
				"bs-watchlist-grid-watchlist-column-hasunreadchanges-header",
				"bs-watchlist-grid-watchlist-column-hasunreadchanges-tooltip",
				"bs-watchlist-grid-watchlist-label-filter",
				"bs-watchlist-grid-watchlist-column-istalkpage-header"
			]
		},
		"ext.bluespice.watchlist.visualEditor": {
			"scripts": [
				"bs.watchlist/util/tag/WatchListDefinition.js"
			],
			"messages": [
				"bs-watchlist-tag-watchlist-desc",
				"bs-watchlist-ve-watchlistinspector-title",
				"bs-watchlist-ve-watchlist-attr-count-label",
				"bs-watchlist-tag-watchlist-attr-count-help",
				"bs-watchlist-ve-watchlist-attr-maxtitlelength-label",
				"bs-watchlist-tag-watchlist-attr-maxtitlelength-help",
				"bs-watchlist-ve-watchlist-attr-order-label",
				"bs-watchlist-tag-watchlist-attr-order-help",
				"bs-watchlist-ve-watchlist-attr-pagename-label",
				"bs-watchlist-tag-watchlist-attr-pagename-help",
				"bs-watchlist-ve-watchlist-attr-time-label",
				"bs-watchlist-tag-watchlist-attr-time-help"
			]
		}
	},
	"ResourceFileModulePaths": {
		"localBasePath": "resources",
		"remoteExtPath": "BlueSpiceWatchList/resources"
	},
	"MessagesDirs": {
		"BSWatchList": [
			"i18n"
		]
	},
	"AutoloadNamespaces": {
		"BlueSpice\\WatchList\\": "src/"
	},
	"load_composer_autoloader": true,
	"manifest_version": 2
}
